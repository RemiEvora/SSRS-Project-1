  <DataSets>
    <DataSet Name="MoviesList">
      <Query>
        <DataSourceName>Movies</DataSourceName>
        <CommandText>SELECT Director.FullName AS Director, Genre.Genre, Certificate.Certificate, Film.Title, Film.ReleaseDate, Film.RunTimeMinutes, Film.OscarWins

FROM Certificate INNER JOIN 
           Film ON Certificate.CertificateID = Film.CertificateID INNER JOIN 
           Director ON Film.DirectorID = Director.DirectorID INNER JOIN 
           Genre ON Film.GenreID = Genre.GenreID</CommandText>
      </Query>
      <Fields>
        <Field Name="Director">
          <DataField>Director</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Genre">
          <DataField>Genre</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Certificate">
          <DataField>Certificate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Title">
          <DataField>Title</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="ReleaseDate">
          <DataField>ReleaseDate</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="RunTimeMinutes">
          <DataField>RunTimeMinutes</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="OscarWins">
          <DataField>OscarWins</DataField>
          <rd:TypeName>System.Byte</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
